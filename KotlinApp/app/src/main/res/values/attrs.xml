        <attr name="defaultPaintColor" format="color"/>
        <attr name="defaultStrokeWidth" format="integer"/>

        <attr name="selectPaintColor" format="color"/>
        <attr name="selectStrokeWidth" format="integer"/>

        <attr name="errorPaintColor" format="color"/>
